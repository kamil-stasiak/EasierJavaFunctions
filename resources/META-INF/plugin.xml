<idea-plugin>
  <id>com.your.company.unique.plugin.id</id>
  <name>Easier Java Functions</name>
  <version>1.0</version>
  <vendor email="kamilstasiaknew@gmail.com" url="https://github.com/kamil-stasiak">Kamil Stasiak</vendor>

  <description>Plugin that change how FunctionalInterfaces looks like in Java</description>

  <change-notes>
    First version of plugin. It replace parameters declaration to java style functions. Version 0.0.1
  </change-notes>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/build_number_ranges.html for description -->
  <idea-version since-build="173.0"/>

  <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
       on how to target different products -->
  <depends>com.intellij.modules.lang</depends>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <lang.foldingBuilder language="JAVA" implementationClass="me.stasiak.SimpleFoldingBuilder"/>
  </extensions>

  <actions>
  </actions>

</idea-plugin>
